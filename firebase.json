{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000,
      "host": "0.0.0.0"
    },
    "ui": {
      "host": "0.0.0.0",
      "enabled": true
    },
    "auth": {
      "port": 9099,
      "host": "0.0.0.0"
    },
    "database": {
      "port": 9000,
      "host": "192.168.1.246"
    },
    "storage": {
      "port": 9199,
      "host": "0.0.0.0"
    }
  },
  "database": {
    "rules": "./database.rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}